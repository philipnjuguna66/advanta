{
    "name": "philipnjuguna/advanta",
    "description": "Advanta sms sdk for laravel",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^2.72.6",
        "nesbot/carbon": "^2.43"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6.33",
        "symfony/dotenv": "^4.3@dev"
    },
    "autoload": {
        "psr-4": {
            "PhilipNjuguna\\Advanta\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Philip Njuguna",
            "email": "philkiangai@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "PhilipNjuguna\\Advanta\\AdvantaServiceProvider"
            ],
            "aliases": {
                "Advanta": "PhilipNjuguna\\advanta\\AdvantaFacade"
            }
        }
    }
}
